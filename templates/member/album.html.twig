{% extends 'base.html.twig' %}

{% block title %}Album Photo - Icom Circus{% endblock %}

{% block body %}

<a href="{{ path('app_member') }}"> <button type="button" class="btn btn-info">Retour au blog de l'espace Parent</button></a>

<h1>Consulter l'album de la session de votre enfant:</h1>

<div class="container">
    <div class="row">
        {% for photo in photos %}

                <div class="col-md-4">
                    <img src="/uploads/{{photo.photoName}}" alt="" class="img-fluid">
                </div>

        {% endfor %}

    </div>
</div>


{% endblock %}
